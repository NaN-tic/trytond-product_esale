<data>
    <xpath expr="/form/field[@name='link']" position="after">
        <label name="esale_available"/>
        <field name="esale_available"/>
    </xpath>
    <xpath expr="/form/notebook/page[@name='description']" position="after">
        <page string="eSale" id="esale">
            <label name="esale_base_image"/>
            <field name="esale_base_image"/>
            <label name="esale_small_image"/>
            <field name="esale_small_image"/>
            <label name="esale_thumbnail"/>
            <field name="esale_thumbnail"/>
            <label name="esale_exclude"/>
            <field name="esale_exclude"/>
            <label name="esale_position"/>
            <field name="esale_position"/>
            <label name="esale_label"/>
            <field name="esale_label"/>
        </page>
    </xpath>
</data>
